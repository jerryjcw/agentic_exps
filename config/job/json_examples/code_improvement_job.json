{
    "job_name": "CodeImprovementAnalysis",
    "job_description": "Analyze Python code for security vulnerabilities, performance bottlenecks, code quality issues, and testing gaps",
    "job_type": "code_analysis",
    "runner_config": {
        "app_name": "CodeImprovementAnalysis",
        "session_config": {
            "user_id": "code_analyzer",
            "session_id": "analysis_session"
        }
    },
    "agent_config": {
        "config_path": "config/agent/json_examples/simple_code_improvement.json",
        "config_type": "file"
    },
    "input_config": {
        "input_file_path": "agent_io/agent_io.py",
        "input_type": "file",
        "language": "Python",
        "framework": "Google ADK",
        "preview_length": 500
    },
    "analysis_config": {
        "user_query_template": "Please analyze the following Python code for improvements:\n\nFile: {file_name}\nLanguage: {language}\nFramework: {framework}\n\nCode to analyze:\n```python\n{code_content}\n```\n\nPlease provide a comprehensive analysis focusing on:\n1. Security vulnerabilities and defensive measures\n2. Performance bottlenecks and optimization opportunities\n3. Code quality issues and maintainability improvements\n4. Testing gaps and coverage recommendations\n\nProvide specific recommendations with examples where possible.",
        "analysis_focus": [
            "Security vulnerabilities and defensive measures",
            "Performance bottlenecks and optimization opportunities",
            "Code quality issues and maintainability improvements",
            "Testing gaps and coverage recommendations"
        ]
    },
    "output_config": {
        "output_directory": "output",
        "output_format": ["txt", "json"],
        "file_naming": "code_analysis_{input_filename}_{timestamp}",
        "timestamp_format": "%Y%m%d_%H%M%S",
        "include_metadata": true,
        "metadata_fields": [
            "analysis_date",
            "input_file",
            "agent_name",
            "events_generated",
            "file_size",
            "lines_of_code"
        ]
    },
    "execution_config": {
        "track_execution_steps": true,
        "display_progress": true,
        "log_level": "INFO",
        "error_handling": "continue_on_agent_failure",
        "timeout_seconds": 300
    },
    "report_config": {
        "include_final_responses": true,
        "include_code_preview": true,
        "include_execution_summary": true,
        "display_results_summary": true
    }
}